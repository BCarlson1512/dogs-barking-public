{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Default",
    "compilerOptions": {
        "target": "es6",
        "downlevelIteration": true,
        "allowJs": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": false,
        "baseUrl": "."
    },
    "exclude": ["node_modules", "*.js", "*.test.ts"]
}
